#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jakekinchen/Documents/moleculens/node_modules/.pnpm/@agentdeskai+browser-tools-server@1.2.1/node_modules/@agentdeskai/browser-tools-server/dist/node_modules:/Users/jakekinchen/Documents/moleculens/node_modules/.pnpm/@agentdeskai+browser-tools-server@1.2.1/node_modules/@agentdeskai/browser-tools-server/node_modules:/Users/jakekinchen/Documents/moleculens/node_modules/.pnpm/@agentdeskai+browser-tools-server@1.2.1/node_modules/@agentdeskai/node_modules:/Users/jakekinchen/Documents/moleculens/node_modules/.pnpm/@agentdeskai+browser-tools-server@1.2.1/node_modules:/Users/jakekinchen/Documents/moleculens/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jakekinchen/Documents/moleculens/node_modules/.pnpm/@agentdeskai+browser-tools-server@1.2.1/node_modules/@agentdeskai/browser-tools-server/dist/node_modules:/Users/jakekinchen/Documents/moleculens/node_modules/.pnpm/@agentdeskai+browser-tools-server@1.2.1/node_modules/@agentdeskai/browser-tools-server/node_modules:/Users/jakekinchen/Documents/moleculens/node_modules/.pnpm/@agentdeskai+browser-tools-server@1.2.1/node_modules/@agentdeskai/node_modules:/Users/jakekinchen/Documents/moleculens/node_modules/.pnpm/@agentdeskai+browser-tools-server@1.2.1/node_modules:/Users/jakekinchen/Documents/moleculens/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@agentdeskai/browser-tools-server/dist/browser-connector.js" "$@"
else
  exec node  "$basedir/../@agentdeskai/browser-tools-server/dist/browser-connector.js" "$@"
fi
